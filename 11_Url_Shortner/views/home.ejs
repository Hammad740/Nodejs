<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>All URLs</title>
  </head>
  <body>
    <h1>All URLs</h1>
    <ul>
      <% for (const url of allUrls) { %>
      <li>
        <a href="<%= url.redirectUrl %>"><%= url.redirectUrl %></a>
        (Visits: <%= url.visitHistory.length %>)
      </li>
      <% } %>
    </ul>
  </body>
</html>
